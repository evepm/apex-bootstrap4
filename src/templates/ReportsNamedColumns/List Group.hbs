{{#row_template_before_rows}}
<div id="report-#REGION_STATIC_ID#" class="list-group #COMPONENT_CSS_CLASSES#">
{{/row_template_before_rows}}

    {{#before_column_heading}}{{/before_column_heading}}

        {{#column_heading_template}}{{/column_heading_template}}

    {{#after_column_heading}}{{/after_column_heading}}

    {{#row_template_before_first}}{{/row_template_before_first}}

        {{#row_template1}}
        <a href="#LINK#" class="list-group-item list-group-item-#STYLE# #ACTIVE# #DISABLED#">
            <span class="label label-#LABEL_STYLE# label-pill pull-xs-right">#LABEL_TEXT#</span>
            <h4 class="list-group-item-heading">#HEADING#</h4>
            <p class="list-group-item-text">#TEXT#</p>
        </a>
        {{/row_template1}}
        {{#row_template_condition1}}:LINK is not null{{/row_template_condition1}}
        {{#row_template_display_cond1}}NOT_CONDITIONAL{{/row_template_display_cond1}}
        {{#row_template2}}
        <div class="list-group-item list-group-item-#STYLE# #ACTIVE# #DISABLED#">
            <span class="label label-#LABEL_STYLE# label-pill pull-xs-right">#LABEL_TEXT#</span>
            <h4 class="list-group-item-heading">#HEADING#</h4>
            <p class="list-group-item-text">#TEXT#</p>
        </div>
        {{/row_template2}}
        {{#row_template_condition2}}{{/row_template_condition2}}
        {{#row_template_display_cond2}}{{/row_template_display_cond2}}
        {{#row_template3}}{{/row_template3}}
        {{#row_template_condition3}}{{/row_template_condition3}}
        {{#row_template_display_cond3}}{{/row_template_display_cond3}}
        {{#row_template4}}{{/row_template4}}
        {{#row_template_condition4}}{{/row_template_condition4}}
        {{#row_template_display_cond4}}{{/row_template_display_cond4}}

    {{#row_template_after_last}}{{/row_template_after_last}}

{{#row_template_after_rows}}</div>{{/row_template_after_rows}}

