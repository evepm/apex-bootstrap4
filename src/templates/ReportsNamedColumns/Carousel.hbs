{{#row_template_before_rows}}
<div id="carousel-#REGION_STATIC_ID#" class="carousel slide" data-ride="carousel">
<ol class="carousel-indicators"></ol>
<div class="carousel-inner" role="listbox">
{{/row_template_before_rows}}

    {{#before_column_heading}}{{/before_column_heading}}

        {{#column_heading_template}}{{/column_heading_template}}

    {{#after_column_heading}}{{/after_column_heading}}

    {{#row_template_before_first}}{{/row_template_before_first}}

        {{#row_template1}}
        <div class="carousel-item active">
            <img src="#IMG_SOURCE#" alt="#IMG_ALT#">
            <div class="carousel-caption">
                <h3>#CAPTION_TITLE#</h3>
                <p>#CAPTION_TEXT#</p>
            </div>
        </div>
        {{/row_template1}}
        {{#row_template_condition1}}#ROWNUM# = 1{{/row_template_condition1}}
        {{#row_template_display_cond1}}NOT_CONDITIONAL{{/row_template_display_cond1}}
        {{#row_template2}}
        <div class="carousel-item">
            <img src="#IMG_SOURCE#" alt="#IMG_ALT#">
            <div class="carousel-caption">
                <h3>#CAPTION_TITLE#</h3>
                <p>#CAPTION_TEXT#</p>
            </div>
        </div>
        {{/row_template2}}
        {{#row_template_condition2}}#ROWNUM# <> 1{{/row_template_condition2}}
        {{#row_template_display_cond2}}NOT_CONDITIONAL{{/row_template_display_cond2}}
        {{#row_template3}}{{/row_template3}}
        {{#row_template_condition3}}{{/row_template_condition3}}
        {{#row_template_display_cond3}}{{/row_template_display_cond3}}
        {{#row_template4}}{{/row_template4}}
        {{#row_template_condition4}}{{/row_template_condition4}}
        {{#row_template_display_cond4}}{{/row_template_display_cond4}}

    {{#row_template_after_last}}{{/row_template_after_last}}

{{#row_template_after_rows}}
<a class="left carousel-control" href="#carousel-#REGION_STATIC_ID#" role="button" data-slide="prev">
    <span class="icon-prev" aria-hidden="true"></span>
    <span class="sr-only">Previous</span>
</a>
<a class="right carousel-control" href="#carousel-#REGION_STATIC_ID#" role="button" data-slide="next">
    <span class="icon-next" aria-hidden="true"></span>
    <span class="sr-only">Next</span>
</a>
</div>
</div>
{{/row_template_after_rows}}

{{#pagination_template}}{{/pagination_template}}
{{#next_page_template}}{{/next_page_template}}
{{#previous_page_template}}{{/previous_page_template}}
{{#next_set_template}}{{/next_set_template}}
{{#previous_set_template}}{{/previous_set_template}}
